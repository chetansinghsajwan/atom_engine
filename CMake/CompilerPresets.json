{
    "version": 4,
    "configurePresets": [
        {
            "name": "compiler-gcc",
            "hidden": true,
            "cacheVariables": {
                "CMAKE_C_COMPILER": "gcc",
                "CMAKE_CXX_COMPILER": "g++"
            }
        },
        {
            "name": "compiler-clang",
            "hidden": true,
            "cacheVariables": {
                "CMAKE_C_COMPILER": "/usr/bin/clang",
                "CMAKE_CXX_COMPILER": "/usr/bin/clang++"
            }
        },
        {
            "name": "compiler-msvc",
            "hidden": true,
            "cacheVariables": {
                "CMAKE_C_COMPILER": "cl",
                "CMAKE_CXX_COMPILER": "cl"
            }
        }
    ],
    "buildPresets": [
        {
            "name": "compiler-gcc",
            "hidden": true
        },
        {
            "name": "compiler-clang",
            "hidden": true
        },
        {
            "name": "compiler-msvc",
            "hidden": true
        }
    ]
}
